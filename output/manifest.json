{
    "manifest_version": 2,
    "name": "Obscrawler",
    "version": "1.0",
    "background": {
        "persistent": true,
        "scripts": [
            "src/defaults.js",
            "src/chaotic-bag.js",
            "src/url-containers.js",
            "src/url-filters.js",
            "src/obscrawler.js",
            "background.js"
        ]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_start"
    }],
    "browser_action": {
        "default_title": "Obscrawler"
    },
    "permissions": [
        "tabs",
        "storage",
        "http://*/",
        "https://*/"
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    }
}
